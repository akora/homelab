# Traefik Environment Variables
# This file is managed by Ansible - local changes will be overwritten

# Cloudflare Credentials (using API Token for DNS challenge)
CLOUDFLARE_EMAIL={{ vault_traefik_cloudflare_email }}
CLOUDFLARE_DNS_API_TOKEN={{ vault_traefik_cloudflare_dns_api_token }}

# Let's Encrypt Email
LETSENCRYPT_EMAIL={{ vault_traefik_letsencrypt_email }}
